Team:

#include<iostream>
using namespace std;
int main()
{
    int x,a,b,c,count=0;
    cin>>x;
    for(int i=0; i<x; i++)
    {
        cin>>a>>b>>c;
        if(a+b+c >=2)
        {
            count++;
        }
    }
    cout<<count<<endl;
}

NEXT ROUND:

#include<iostream>
using namespace std;
int main()
{
    int n, k, i, a[100], count = 0;
    cin >> n >> k;
    for(i=0; i<n; i++)
    {
        cin >> a[i];
    }
    for(i = 0; i < n; i++)
    {
        if(a[i] > 0 && a[i] >= a[k - 1])
            count++;
    }
    cout << count << endl;
}
